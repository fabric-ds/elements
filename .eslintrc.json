{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unexpected-multiline": "off"
  }
}
